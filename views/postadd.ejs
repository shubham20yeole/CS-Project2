  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">


            <div class="aa-properties-details">
            <h1>Post New Property</h1>
            <table width="100%">
            <tr>
            <td>
            <input type="file" name="file" id="file"/></td><td><input type="file" name="file" id="file2"/><input type="hidden" id="image2" name="image2"></td></tr>
            <tr>
            <td>
            <input type="file" name="file" id="file3"/><input type="hidden" id="image3" name="image3"></td>
            <td><input type="file" name="file" id="file4"/><input type="hidden" id="image4" name="image4"></td></tr></table>
          
          
           <form method="post" action="postproperty">
                <input type="text" name="timestamp" id="timestamp" placeholder="timestamp"><br><br>
                <input type="hidden" id="image1" name="image1">
                <input type="text" name="phone" placeholder="Phone">
                <input type="text" name="email" placeholder="Email">
                <input type="text" name="telephone" placeholder="Telephone"><br><br>


                <input type="text" name="posttime" id="posttime" placeholder="Street Address"><br><br>
                <input type="text" name="staddress" placeholder="Street Address">
                <input type="text" name="city" placeholder=" city">
                <input type="text" name="state" placeholder=" state"><br><br>
                <input type="text" name="zip" placeholder=" zip">
                <input type="text" name="county" placeholder=" county">
                <input type="text" name="country" placeholder=" country"><br><br>

                <select id="categoryselect">
                  <option value="Other">Category</option>
                  <option value="Houses">Houses</option>
                  <option value="Appartments">Appartment</option>
                  <option value="Shop">Shop</option>
                  <option value="Paying Guest">Paying Guest</option>
                  <option value="Town Home">Town Home</option>
                  <option value="Farm House">Farm House</option>
                </select>
                <select id="addselect">
                  <option value="Sale">Type</option>
                  <option value="Sale">For Sale</option>
                  <option value="Rent">For Rent</option>
                </select><br><br>
                <input type="number" name="bedroom" placeholder="Bedroom">
                <input type="number" name="kitchen" placeholder="Kitchen">
                <input type="number" name="bathroom" placeholder="Bathroom"><br><br>
                <input type="hidden" name="propertytype" id="propertytype" placeholder="propertytype">
                <input type="hidden" name="addtype" id="addtype" placeholder="propertytype"><br><br>
                <input type="number" name="area" placeholder="Area in sq feets">
                <input type="number" name="cost" placeholder="Price in USD">
                <input type="text" name="discription" placeholder="Discription"><br><br>

                <input type="submit" style="display:none;" id="submitForm" value="Post Add">
              </form>
              <button id="uploadimage">Save Post</button><div id="loader"></div>

              <div id="lodardiv"></div>
              <div id="div1" style="display:none;">Upload image 1: <b>Successfull</b></div>
              <div id="div2" style="display:none;">Upload image 2: <b>Successfull</b></div>
              <div id="div3" style="display:none;">Upload image 3: <b>Successfull</b></div>
              <div id="div4" style="display:none;">Upload image 4: <b>Successfull</b></div>
              <div id="div5" style="display:none;">Submitting - Property : <b>DONE</b></div><br>
            </div>          
          </div>
