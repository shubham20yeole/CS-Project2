<body>

<div class="main">
<h1>SET NEW PASSWORD</h1>
		<div class="wthree_top_forms">
			
			<div class="agile-info_w3ls agile-info_w3ls_sub hvr-buzz-out">
				<h3>Login</h3>
				<div class="agile-info_w3ls_grid second">
  					<form method="POST" action="/login">
      					<input type="email" placeholder="Enter Username" name="email" required>
      					<input type="password" placeholder="Enter Password" name="password" required>
					
					<div class="agile_remember">
						<div class="agile_remember_left">
							<div class="check">
								<label class="checkbox"><input type="checkbox" name="checkbox"><i> </i>remember me</label>
							</div>
						</div>
						<div class="agile_remember_right">
							<a href="#">Forgot Password?</a>
						</div>
						<div class="clear"> </div>
					</div>
						<input type="submit" value="Sign In">
					</form>
					<h4>Continue With</h4>
					<div class="social_icons agileinfo">
					   <section class="social">    
					   <% include partials/fboauth %>
					   <br><br><p style="font-size: 23px; color: white">Login with Facebook</p>
						</section>
					</div>
					<h5>Don't have an account? <a href="#">Sign Up</a></h5>
				</div>
			</div>
			<div class="agile-info_w3ls third hvr-buzz-out">
				<h3>Reset Password</h3>
				<div class="agile-info_w3ls_grid third">
								<h1>Reset Password</h1>

					<form action="/resetpassword" method="post">
						<input type="email" name="email" placeholder="Enter email address" required=" ">
						
						<p class="asd"><a href="#">Great passwords</a> use upper and lower case
							characters, numbers and symbols like "!@#$*.</p>
						<input type="submit" value="RESET">
					</form>
					<h5 class="rest">Already a member? <a href="#">Sign In</a></h5><br><br>
				</div>
			</div>
			<div class="clear"> </div>
		</div>

    <script type="text/javascript" src="theme/js/signupin.js"></script>

