            <!-- Start Single properties sidebar -->
            <div class="aa-properties-single-sidebar">
              <h3>Latest Properties</h3>
              <% latestproperty.forEach(function(latestproperty){%>
                 <div class="media">
                  <div class="media-left">
                    <a href="../detailedproperty/<%= latestproperty.timestamp %>" target="_blank">
                      <img class="media-object" src="<%= latestproperty.image1 %>" alt="img">
                    </a>
                  </div>
                  <div class="media-body">
                    <h4 class="media-heading"><a href="../detailedproperty/<%= latestproperty.timestamp %>" target="_blank"><%= latestproperty.title %></a></h4>
                    <p><%= latestproperty.discription %></p>                
                    <p class="address">

                    <a type="button" id="mapb" href="#popup1" onclick="init_map(<%= latestproperty.latitude %>,<%= latestproperty.longitude %>,'<h3><%= property.title %></h3><h6> Property Location</h6>')"><img src="../images/mappin.jpg" width="22" height="32"></a>


                    &nbsp&nbsp&nbsp<%= latestproperty.staddress %>, <%= latestproperty.city %>, 
                    <%= latestproperty.state %>, <%= latestproperty.zip %>, <%= latestproperty.country %>
                      
                    </p>                
                    <p class="cost">$<%= latestproperty.cost %></p>
                    <p class="readmore"><a href="../detailedproperty/<%= latestproperty.timestamp %>" target="_blank">READ MORE
                    </a></p>
                  </div>              
                </div>
              <%}) %>
              
